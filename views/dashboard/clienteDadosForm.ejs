<%- include('../partials/head',{title:'BCU Dados Pessoais'})%>
  <%- include('../partials/dashboardHeader')%>
    <section class="dashboardUserDadosFormContainer">
      <form class="dashboardUserDadosForm borderCurveElements">
        <div class="informacoesPessoais">
          <label for="nomeCompleto">Nome Completo</label>
          <input type="text"  id="nomeCompleto">
          <label for="bilhete">Bilhete</label>
          <input type="text" id="bilhete">
          <label for="nascimento">Data de Nascimento</label>
          <input type="date" id="nascimento">
          <label for="estadoCivil">Estado Civil</label>
          <select name="estado" id="estadoCivil">
            <option value="">Solteiro</option>
            <option value="">Casado</option>
            <option value="">Viuvo(a)</option>
          </select>
          <label for="sexo"></label>
          <select name="sexo" id="sexo">
            <option value="">Masculino</option>
            <option value="">Femenino</option>
          </select>
        </div>

        <div class="informacoesAcademicas">
          <label for="nivelAcademico">Nível Academico</label>
          <select name="nivelAcademico" id="nivelAcademico">
            <option value="base">Base</option>
            <option value="médio">Médio</option>
            <option value="licenciado">Ensino Superior</option>
            <option value="mestre">Mestre</option>
            <option value="doctor">Doctor</option>
          </select>
          <label for="tel1">Contacto Principal</label>
          <input type="tel" name="contactos" id="tel1">
          <label for="tel2">Contacto Secundário</label>
          <input type="tel" name="contactos" id="tel2">
          <label for="fotoPerfil">
            Foto de Perfil
          </label>
          <input type="file" name="foto" id="fotoPerfil">
          <label for="bilheteFile">Carregar o Bilhete</label>
          <input type="file" name="BI" id="bilheteFile">
          <button class="btnSalvarDados">Salvar</button>
        </div>
      </form>
      <%- include('../partials/sideBar')%>
    </section>
    <%- include('../partials/footer')%>
      <%- include('../partials/pageEnd')%>